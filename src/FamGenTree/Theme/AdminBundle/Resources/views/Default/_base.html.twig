<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>DASHGUM - FREE Bootstrap Admin Template</title>

    <!-- Bootstrap core CSS -->
    <link href="{{ asset('bundles/famgentreethemeadmin/css/bootstrap.css') }}" rel="stylesheet">
    <!--external css-->
    <link href="{{ asset('bundles/famgentreethemeadmin/font-awesome/css/font-awesome.css') }}" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/famgentreethemeadmin/css/zabuto_calendar.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/famgentreethemeadmin/js/gritter/css/jquery.gritter.css')}}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/famgentreethemeadmin/lineicons/style.css') }}">

    <!-- Custom styles for this template -->
    <link href="{{ asset('bundles/famgentreethemeadmin/css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/famgentreethemeadmin/css/style-responsive.css') }}" rel="stylesheet">

    {% block head_styles %}
    {% endblock %}

    <script src="{{ asset('bundles/famgentreethemeadmin/js/chart-master/Chart.js') }}"></script>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="{{ asset('lib/html5shiv/html5shiv.min.js') }}"></script>
    <script src="{{ asset('lib/respond-1.4.2/respond.min.js') }}"></script>
    <![endif]-->
</head>
<body>
{% block body %}
    <section id="container" >
        <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        <!--header start-->
        {% include 'FamGenTreeThemeAdminBundle:Example:_part.header.html.twig' %}
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        {% include 'FamGenTreeThemeAdminBundle:Example:_part.side_menu.html.twig' with {'active': 'general'} %}
        <!--sidebar end-->

        <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
        {% block main_content %}
        {% endblock %}
        </section>
        <!--main content end-->

        <!--footer start-->
        {% block footer %}
            <footer class="site-footer">
                <div class="text-center">
                    2015 - Christoph Graupner
                    <a href="#top" class="go-top">
                        <i class="fa fa-angle-up"></i>
                    </a>
                </div>
            </footer>
        {% endblock %}
        <!--footer end-->
    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="{{ asset('bundles/famgentreethemeadmin/js/jquery.js') }}"></script>
    <script src="{{ asset('bundles/famgentreethemeadmin/js/jquery-1.8.3.min.js') }}"></script>
    <script src="{{ asset('bundles/famgentreethemeadmin/js/bootstrap.min.js') }}"></script>
    <script class="include" type="text/javascript"
            src="{{ asset('bundles/famgentreethemeadmin/js/jquery.dcjqaccordion.2.7.js') }}"></script>
    <script src="{{ asset('bundles/famgentreethemeadmin/js/jquery.scrollTo.min.js') }}"></script>
    <script src="{{ asset('bundles/famgentreethemeadmin/js/jquery.nicescroll.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/famgentreethemeadmin/js/jquery.sparkline.js') }}"></script>


    <!--common script for all pages-->
    <script src="{{ asset('bundles/famgentreethemeadmin/js/common-scripts.js') }}"></script>

    <script type="text/javascript"
            src="{{ asset('bundles/famgentreethemeadmin/js/gritter/js/jquery.gritter.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/famgentreethemeadmin/js/gritter-conf.js') }}"></script>

    {% block footer_javascript %}
    {% endblock %}
{% endblock %}
</body>
</html>